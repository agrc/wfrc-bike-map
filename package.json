{
  "name": "wfrc-bike-map",
  "version": "1.1.8",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev:firebase-emulators": "./scripts/start-emulators.sh",
    "dev:vite": "vite",
    "download-configs:dev": "npx firebase-tools remoteconfig:get -o src/remote_config_defaults.json --project ut-dts-agrc-wfrc-bike-map-dev",
    "download-configs:prod": "npx firebase-tools remoteconfig:get -o src/remote_config_defaults.json --project ut-dts-agrc-wfrc-bike-map-prod",
    "format": "prettier . --write",
    "postinstall": "pnpm run copy:arcgis",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently 'cd functions && pnpm run build:watch' 'wait-on http://127.0.0.1:4000 && pnpm run dev:vite' 'pnpm run dev:firebase-emulators'",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.34.8",
    "@arcgis/map-components": "^4.34.9",
    "@esri/arcgis-rest-request": "^4.8.0",
    "@esri/calcite-components": "^3.3.3",
    "@tanstack/react-query": "^5.90.20",
    "@ugrc/esri-theme-toggle": "^1.1.2",
    "@ugrc/utah-design-system": "^1.34.2",
    "@ugrc/utilities": "^3.1.1",
    "concurrently": "^9.2.1",
    "firebase": "^12.8.0",
    "immer": "^11.1.3",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-aria": "^3.45.0",
    "react-aria-components": "^1.15.0",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-stately": "^3.43.0",
    "tailwind-merge": "^3.4.0",
    "use-immer": "^0.11.0",
    "usehooks-ts": "^3.1.1",
    "wait-on": "^9.0.3"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@ugrc/eslint-config": "^1.2.3",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.39.2",
    "firebase-tools": "^15.6.0",
    "happy-dom": "^18.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^4.0.18"
  }
}
