{
  "name": "wfrc-bike-map",
  "version": "0.4.0-10",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev:firebase-emulators": "firebase emulators:start --only functions",
    "dev:vite": "vite",
    "download-configs:dev": "npx firebase-tools remoteconfig:get -o src/remote_config_defaults.json --project ut-dts-agrc-wfrc-bike-map-dev",
    "download-configs:prod": "npx firebase-tools remoteconfig:get -o src/remote_config_defaults.json --project ut-dts-agrc-wfrc-bike-map-prod",
    "format": "prettier . --write",
    "postinstall": "pnpm run copy:arcgis",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently 'cd functions && pnpm run build:watch' 'wait-on http://127.0.0.1:4000 && pnpm run dev:vite' 'pnpm run dev:firebase-emulators'",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.32.8",
    "@arcgis/map-components": "^4.32.11",
    "@esri/arcgis-rest-request": "^4.3.0",
    "@tanstack/react-query": "^5.68.0",
    "@ugrc/esri-theme-toggle": "^1.1.0",
    "@ugrc/utah-design-system": "^1.20.3",
    "@ugrc/utilities": "^3.0.2",
    "concurrently": "^9.1.2",
    "firebase": "^11.4.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.479.0",
    "react": "^19.0.0",
    "react-aria": "^3.38.1",
    "react-aria-components": "^1.7.1",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-stately": "^3.36.1",
    "use-immer": "^0.11.0",
    "usehooks-ts": "^3.1.1",
    "wait-on": "^8.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^3.0.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
