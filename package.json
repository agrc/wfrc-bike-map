{
  "name": "wfrc-bike-map",
  "version": "1.1.4",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev:firebase-emulators": "firebase emulators:start --only functions",
    "dev:vite": "vite",
    "download-configs:dev": "npx firebase-tools remoteconfig:get -o src/remote_config_defaults.json --project ut-dts-agrc-wfrc-bike-map-dev",
    "download-configs:prod": "npx firebase-tools remoteconfig:get -o src/remote_config_defaults.json --project ut-dts-agrc-wfrc-bike-map-prod",
    "format": "prettier . --write",
    "postinstall": "pnpm run copy:arcgis",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently 'cd functions && pnpm run build:watch' 'wait-on http://127.0.0.1:4000 && pnpm run dev:vite' 'pnpm run dev:firebase-emulators'",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.33.10",
    "@arcgis/map-components": "^4.33.11",
    "@esri/arcgis-rest-request": "^4.6.0",
    "@esri/calcite-components": "^3.2.1",
    "@tanstack/react-query": "^5.83.0",
    "@ugrc/esri-theme-toggle": "^1.1.1",
    "@ugrc/utah-design-system": "^1.27.3",
    "@ugrc/utilities": "^3.0.4",
    "concurrently": "^9.2.0",
    "firebase": "^11.10.0",
    "immer": "^10.1.1",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-aria": "^3.41.1",
    "react-aria-components": "^1.10.1",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-stately": "^3.39.0",
    "tailwind-merge": "^3.3.1",
    "use-immer": "^0.11.0",
    "usehooks-ts": "^3.1.1",
    "wait-on": "^8.0.4"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@ugrc/eslint-config": "^1.2.3",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.31.0",
    "happy-dom": "^17.6.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^3.2.4"
  }
}
