{
  "name": "wfrc-bike-map",
  "version": "0.4.0-17",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev:firebase-emulators": "firebase emulators:start --only functions",
    "dev:vite": "vite",
    "download-configs:dev": "npx firebase-tools remoteconfig:get -o src/remote_config_defaults.json --project ut-dts-agrc-wfrc-bike-map-dev",
    "download-configs:prod": "npx firebase-tools remoteconfig:get -o src/remote_config_defaults.json --project ut-dts-agrc-wfrc-bike-map-prod",
    "format": "prettier . --write",
    "postinstall": "pnpm run copy:arcgis",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently 'cd functions && pnpm run build:watch' 'wait-on http://127.0.0.1:4000 && pnpm run dev:vite' 'pnpm run dev:firebase-emulators'",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.30.5",
    "@esri/calcite-components": "^2.7.1"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@ugrc/eslint-config": "^1.2.1",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "cpy-cli": "^5.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-packagejson": "^2.5.0",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.3",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^2.1.1"
  }
}
